{
    "name": "indigophp/proton-crud",
    "description": "Proton CRUD skeleton",
    "license": "MIT",
    "keywords": ["proton", "CRUD"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/sagikazarmark/tactician-container"
        }
    ],
    "require": {
        "php": ">=5.4",
        "twig/twig": "~1.0",
        "indigophp/crud": "dev-master",
        "indigophp/crud-doctrine": "dev-master",
        "league/tactician-container": "dev-cleanup as 0.1",
        "fuelphp/fieldset": "dev-master",
        "fuelphp/validation": "dev-master"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1",
        "henrikbjorn/phpspec-code-coverage" : "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Proton\\Crud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Proton\\Crud\\Stub\\": "stubs/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
